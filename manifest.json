{
  "manifest_version": 3,
  "name": "Tab Timer",
  "description": "Keep track of how long a tab has been opened for.",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "tabs"],
  "icons": {
    "128": "images/icon.png"
  }
}
